{
  "name": "Harambro",
  "version": "1.0",
  "description": "First Extension!",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "<all_urls>",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "options_page": "options.html",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/heli_logo_16.png",
      "32": "images/heli_logo_32.png",
      "48": "images/heli_logo_48.png",
      "128": "images/heli_logo_128.png"
    }
  },
  "icons": {
    "16": "images/heli_logo_16.png",
    "32": "images/heli_logo_32.png",
    "48": "images/heli_logo_48.png",
    "128": "images/heli_logo_128.png"
  },
  "manifest_version": 2
}